{
  "name": "backend.tensorify.io",
  "private": true,
  "workspaces": [
    "packages/cli",
    "packages/sdk/v1",
    "packages/plugin-engine",
    "packages/shared",
    "packages/transpiler",
    "packages/create-tensorify-plugin/v1",
    "services/api",
    "services/plugins.tensorify.io"
  ],
  "scripts": {
    "dev": "turbo run dev --filter='./services/api' --filter='./services/plugins.tensorify.io' --parallel",
    "dev:services:api": "turbo run build --filter='./packages/plugin-engine' && turbo run dev --filter='./packages/plugin-engine' --filter='./services/api' --parallel",
    "dev:packages:plugin-engine": "turbo run dev --filter='./packages/plugin-engine'",
    "dev:packages:sdk": "turbo run dev --filter='./packages/sdk/v1'",
    "dev:packages:transpiler": "turbo run dev --filter='./packages/transpiler'",
    "dev:packages:shared": "turbo run dev --filter='./packages/shared'",
    "dev:packages:create-tensorify-plugin": "turbo run dev --filter='./packages/create-tensorify-plugin/v1'",
    "dev:packages:cli": "turbo run dev --filter='./packages/cli'",
    "dev:services:plugins.tensorify.io": "turbo run dev --filter='./services/plugins.tensorify.io'",
    "build": "turbo run build",
    "build:cli": "turbo run build --filter='./packages/cli'",
    "cli": "npm run build:cli && node packages/cli/lib/bin/tensorify.js",
    "tensorify": "npm run build:cli && node packages/cli/lib/bin/tensorify.js",
    "test": "turbo run test",
    "version:patch": "npm version patch --workspace=packages/cli --workspace=packages/sdk --workspace=packages/create-tensorify-plugin",
    "version:minor": "npm version minor --workspace=packages/cli --workspace=packages/sdk --workspace=packages/create-tensorify-plugin",
    "version:major": "npm version major --workspace=packages/cli --workspace=packages/sdk --workspace=packages/create-tensorify-plugin",
    "publish:all": "npm run build && npm publish --workspace=packages/cli --workspace=packages/sdk --workspace=packages/create-tensorify-plugin",
    "versions": "./scripts/version-management.sh show",
    "versions:patch": "./scripts/version-management.sh patch",
    "versions:minor": "./scripts/version-management.sh minor",
    "versions:major": "./scripts/version-management.sh major",
    "versions:sync": "./scripts/version-management.sh sync",
    "versions:build": "./scripts/version-management.sh build",
    "versions:publish": "./scripts/version-management.sh publish",
    "versions:release": "./scripts/version-management.sh release"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "npm@10.8.1"
}
