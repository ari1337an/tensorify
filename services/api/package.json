{
  "name": "@tensorify.io/backend-api",
  "version": "1.0.0",
  "private": true,
  "description": "The backend transpiler endpoint for generating the codes and fetching plugin metadata for Tensorify.",
  "main": "dist/index.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@clerk/backend": "^2.4.1",
    "@tensorify.io/plugin-engine": "workspace:*",
    "@tensorify.io/transpiler": "workspace:*",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "@tus/s3-store": "^2.0.0",
    "@tus/server": "^2.2.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^9.0.8",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^2.0.1",
    "swagger-ui-express": "^5.0.1",
    "timeago.js": "^4.0.2",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.25.76",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/body-parser": "^1.19.5",
    "jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "@repo/eslint-config": "workspace:*"
  },
  "scripts": {
    "generate-indexes": "node ./scripts/generate_indexes.cjs",
    "build": "pnpm run generate-indexes && tsc",
    "dev": "pnpm run generate-indexes && NODE_OPTIONS=\"--no-node-snapshot\" ts-node-dev --respawn --transpile-only --no-notify --exit-child --ignore-watch node_modules src/index.ts",
    "start": "node --no-node-snapshot dist/index.js",
    "test": "pnpm run generate-indexes && jest --passWithNoTests",
    "prepublishOnly": "echo 'Error: This package cannot be published' && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlphaWolf-Ventures/backend.tensorify.io.git"
  },
  "keywords": [
    "tensorify",
    "backend",
    "transpiler",
    "api-endpoint",
    "plugin",
    "metadata",
    "restapi",
    "api"
  ],
  "author": "Arian S. Hasan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/AlphaWolf-Ventures/backend.tensorify.io/issues"
  },
  "homepage": "https://github.com/AlphaWolf-Ventures/backend.tensorify.io#readme",
  "publishConfig": {
    "access": "restricted"
  }
