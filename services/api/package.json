{
  "name": "@tensorify.io/backend-api-v1",
  "version": "1.0.0",
  "private": true,
  "description": "The backend transpiler endpoint for generating the codes and fetching plugin metadata for Tensorify.",
  "main": "dist/index.js",
  "dependencies": {
    "@tensorify.io/sdk": "*",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "pm2": "^5.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "generate-indexes": "node ./scripts/generate_indexes.cjs",
    "build": "npm run generate-indexes && tsc",
    "dev": "npm run generate-indexes && ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "npm run generate-indexes && jest",
    "prepublishOnly": "echo 'Error: This package cannot be published' && exit 1",
    
    "start:pm2": "npm run build && pm2 start ./ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop tensorify-api",
    "restart:pm2": "pm2 restart tensorify-api",
    "reload:pm2": "npm run build && pm2 reload tensorify-api",
    "delete:pm2": "pm2 delete tensorify-api",
    "logs:pm2": "pm2 logs tensorify-api",
    "monit:pm2": "pm2 monit",
    "status:pm2": "pm2 status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlphaWolf-Ventures/backend.tensorify.io.git"
  },
  "keywords": [
    "tensorify",
    "backend",
    "transpiler",
    "api-endpoint",
    "plugin",
    "metadata",
    "restapi",
    "api"
  ],
  "author": "Arian S. Hasan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/AlphaWolf-Ventures/backend.tensorify.io/issues"
  },
  "homepage": "https://github.com/AlphaWolf-Ventures/backend.tensorify.io#readme",
  "publishConfig": {
    "access": "restricted"
  }
}
