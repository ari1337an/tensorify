  "name": "@tensorify.io/services-app-tensorify-io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch:tsrest\" \"npm run watch:tsrest:client\" \"npm run generate:tsrest; npm run generate:tsrest:client; next dev\"",
    "prebuild": "npm run generate:tsrest && npm run generate:tsrest:client && npx prisma generate",
    "build": "npm run generate:tsrest && npm run generate:tsrest:client && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma": "npx prisma generate && npm run prisma:seed",
    "seed": "node --experimental-specifier-resolution=node src/server/database/prisma/seed.js",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npm run seed",
    "generate:tsrest": "node scripts/generate-tsrest-index.cjs",
    "generate:tsrest:client": "node scripts/generate-tsrest-client.cjs",
    "watch:tsrest": "node scripts/watch-tsrest.cjs",
    "watch:tsrest:client": "node scripts/watch-tsrest-client.cjs",
    "docker:up": "docker compose -f docker-compose.test.yml up -d",
    "docker:down": "docker compose -f docker-compose.test.yml down",
    "migrate:postgres:test": "dotenv -e .env.test -- npx prisma db push --force-reset",
    "test": "npm run generate:tsrest; npm run generate:tsrest:client; npm run docker:up; npm run prisma:generate; npm run migrate:postgres:test; env-cmd -f .env.test jest; npm run docker:down",
    "test:args": "npm run generate:tsrest; npm run generate:tsrest:client; npm run docker:up; npm run prisma:generate; npm run migrate:postgres:test; env-cmd -f .env.test jest --runInBand --verbose"
  },
