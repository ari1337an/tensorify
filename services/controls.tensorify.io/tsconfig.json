{
  "extends": "../../packages/config/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@enterprise/*": ["./src/app/(protected)/(enterprise)/*"],
      "@canvas/*": ["./src/app/(protected)/(canvas)/*"],
      "@app/*": ["./src/app/*"]
    },
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts"
  ]
}
