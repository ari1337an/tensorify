{
  "mcpServers": {
    "tensorify-s3-plugin": {
      "command": "node",
      "args": [
        "/absolute/path/to/backend.tensorify.io/services/mcp/dist/index.js"
      ],
      "env": {
        "S3_ACCESS_KEY_ID": "your_s3_access_key_here",
        "S3_SECRET_ACCESS_KEY": "your_s3_secret_key_here",
        "S3_ENDPOINT": "https://s3.amazonaws.com",
        "S3_REGION": "us-east-1",
        "S3_BUCKET_NAME": "your-tensorify-plugins-bucket",
        "DEBUG_LOGGING": "true"
      }
    }
  }
} 