{
    "tasks": {
      "prebuild": {
        "cache": false,
        "dependsOn": []
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "lib/**", ".next/**", "build/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true,
        "dependsOn": ["^build"]
      },
      "test": {
        "dependsOn": ["build"],
        "outputs": []
      },
      "generate:tsrest": {
        "cache": false,
        "outputs": ["src/app/api/v1/_contracts/index.ts"]
      },
      "generate:tsrest:client": {
        "cache": false,
        "outputs": ["src/app/api/v1/_client/client.ts"]
      },
      "prisma": {
        "cache": false
      },
      "lint": {
        "outputs": []
      }
    }
  }